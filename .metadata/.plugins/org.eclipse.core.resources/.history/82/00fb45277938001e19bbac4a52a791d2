
public class Check_Which_Vowel_Not_Present {

	public static void main(String[] args) {
		String s="Lalitous";
		s.toLowerCase();
		int count=0;
		for(int i=0;i<s.indexOf(s);i++) {
			char c=s.charAt(i);
			   if(c=='a') {
				   count++;
			   }
				   
			   
			}
		System.out.println(count);
	}
		
		
}



public class Check_No_Morethan_3time_find {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int ID=12888776;
		int rem = 0;
		int count = 0;
		while(ID!=0)
		{
			rem =ID%10;
			if(rem==8)
			{
				count++;
			}
			ID=ID/10;
		}
		System.out.println(count>3?"Lucky":"Unlucky");

	}

}

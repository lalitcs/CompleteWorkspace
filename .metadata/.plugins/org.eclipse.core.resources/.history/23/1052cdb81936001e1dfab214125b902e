
public class RemoveExtraSpace {

	public static void main(String[] args) {
		String s="Hello Hi   Bye    Welcome Hi       Bye";
		String ans="          ";
		for(int i=0;i<s.length();i++) {
			if(s.charAt(i)==' '&& s.charAt(i+1)==' ') {
				continue;
			}
			ans=ans+s.charAt(i);
		}
		System.out.println(ans);

	}

}
